{
    "name": "uDAQC",
    "keywords": "device, control",
    "description": "A library for creating a uDAQC device. This allows data acquisition from and control of a microcontroller through the uDAQC architecture",
    "authors":
    {
        "name": "Tyler Moore",
        "url": "https://github.com/overtone1000/uDAQC/"
    },
    "repository":
    {
        "type": "git",
        "url": "https://github.com/overtone1000/uDAQC.git"
    },
    "include": "Components/Device",
    "export": {
        "include": "uDAQC"
    },
    "build":
    {
        "srcFilter":
        [
            "+<uDAQC.cpp>",
            "+<uDAQC.h>"
        ]
    },
    "version": "0.3.0",
    "frameworks": "arduino",
    "platforms": "esp8266"
}
